{
  "manifest_version": 3,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WENMOON: 1-Click Token Launcher",
  "short_name": "WENMOON",
  "description": "Wenmoon is a revolutionary token launcher on Solana.",
  "version": "0.0.1",
  "author": "WENMOON",
  "homepage_url": "https://wenmoon.gg/",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html",
    "default_title": "WENMOON: 1-Click Token Launcher"
  },

  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "https://x.com/*", "https://api.wenmoon.gg/*"],
      "js": ["js/vendor.js"]
    },
    {
      "matches": ["https://twitter.com/*", "https://x.com/*", "https://api.wenmoon.gg/*"],
      "js": ["js/content_script.js"],
      "run_at": "document_end"
    }
  ],

  "background": {
    "service_worker": "js/background.js",
    "type": "module",
    "persistence": true
  },

  "permissions": ["storage", "activeTab", "scripting", "tabs", "webNavigation", "sidePanel"],

  "host_permissions": ["https://twitter.com/*", "https://x.com/*", "https://api.wenmoon.gg/*"],

  "side_panel": {
    "default_path": "sidepanel.html"
  },

  "web_accessible_resources": [
    {
      "resources": ["js/injected.js"],
      "matches": ["https://twitter.com/*", "https://x.com/*", "https://api.wenmoon.gg/*"]
    },
    {
      "resources": ["bs58.bundle.js"],
      "matches": ["https://twitter.com/*", "https://x.com/*", "https://api.wenmoon.gg/*"]
    },
    {
      "resources": ["js/fun_connect.js"],
      "matches": ["https://twitter.com/*", "https://x.com/*", "https://api.wenmoon.gg/*"]
    },
    {
      "resources": ["icons/*"],
      "matches": ["https://twitter.com/*", "https://x.com/*", "https://api.wenmoon.gg/*"]
    }
  ]
}
